{
  "name": "fast-sort-lens",
  "version": "0.0.2",
  "description": "API wrapper around fast-sort",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "vitest run",
    "prepare": "husky",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/fast-sort-lens.git"
  },
  "keywords": [
    "sort",
    "order-by",
    "orderBy",
    "fast-sort",
    "fast-sort-wrapper",
    "finder-sort"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/fast-sort-lens/issues"
  },
  "homepage": "https://github.com/magicdawn/fast-sort-lens#readme",
  "packageManager": "pnpm@10.7.0+sha512.6b865ad4b62a1d9842b61d674a393903b871d9244954f652b8842c2b553c72176b278f64c463e52d40fff8aba385c235c8c9ecf5cc7de4fd78b8bb6d49633ab6",
  "devDependencies": {
    "@magicdawn/finder-sort": "^0.4.0",
    "@magicdawn/prettier-config": "^0.0.4",
    "@swc/core": "^1.11.13",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^3.0.9",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "fast-sort": "^3.4.1"
  }
}
