{
  "name": "fast-sort-lens",
  "version": "0.0.1",
  "description": "API wrapper around fast-sort",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "vitest run",
    "prepare": "husky",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/fast-sort-lens.git"
  },
  "keywords": [
    "sort",
    "order-by",
    "orderBy",
    "fast-sort",
    "fast-sort-wrapper",
    "finder-sort"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/fast-sort-lens/issues"
  },
  "homepage": "https://github.com/magicdawn/fast-sort-lens#readme",
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf",
  "devDependencies": {
    "@magicdawn/finder-sort": "^0.4.0",
    "@magicdawn/prettier-config": "^0.0.4",
    "@swc/core": "^1.10.3",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "fast-sort": "^3.4.1"
  }
}
