    "@typescript-eslint/parser": "^7.13.0",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5",
    "vitest": "^0.34.1",
    "tsup": "^8.1.0"
